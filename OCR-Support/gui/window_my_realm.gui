									flowcontainer = {
										visible = "[Isnt('crown_law')]"
										layoutpolicy_horizontal = expanding
										direction = vertical
										datamodel = "[GuiLawGroup.GetLaws]"

										item = {
											button_text = {
												datacontext = "[GuiLaw.GetLaw]"
