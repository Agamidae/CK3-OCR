								raw_text = "[Localize('VASSAL_CONTRACT_NOT_DEFAULT')]."
								visible = "[Not( VassalContract.IsContractDefault )]"
							}
						}
					}

					text_single = {
						layoutpolicy_horizontal = expanding
						raw_text = "[GetDataModelSize(GetPlayer.GetVassals)] vassals."
