						raw_text = "#D NEEDS TEXT#!"
					}
				}

				block "remaining_items" {
				}

				expand = { }
			}
		}

		vbox = {
			layoutpolicy_horizontal = expanding
			block "hide_items" { }



			block "gridbox" {
				vbox = {
					layoutpolicy_horizontal = expanding
					# flipdirection = yes
					# addcolumn = 85
					# addrow = 90
					# datamodel_wrap = 7

					block "gridbox_slots"
					{
						# maxverticalslots = 1
					}

